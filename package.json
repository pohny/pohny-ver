{
  "name": "pohny-ver",
  "version": "0.0.3",
  "scripts": {
    "build": "coffee --compile --output build src",
    "watch": "coffee --compile --watch --output build src",
    "server": "node ./bin/www",
    "test": "mocha test/*",
    "build-docker-image": "docker build -t incubatio/pohny-ver -f Dockerfile  .",
    "run-docker-image": "docker run --env-file etc/env.sh --network pohny-net incubatio/pohny-ver:latest",
    "rm-docker-image": "docker rm --force $(docker ps -q --filter ancestor=\"incubatio/pohny-ver:latest\")"
  },
  "dependencies": {
    "bluebird": "*",
    "body-parser": "*",
    "bson": "*",
    "coffeescript": "*",
    "debug": "*",
    "express": "^4.18.2",
    "ioredis": "*",
    "joi": "*",
    "jsonrpc-simple": "*",
    "jwt-simple": "*",
    "lodash": "*",
    "mongodb": "^4.1.0",
    "morgan": "*",
    "om-nomnom": "0.0.5",
    "request-promise": "*",
    "requirejs": "*",
    "twilio": "2.11.1",
    "websocket": "^1.0.23"
  },
  "engines": {
    "node": "v16.14.1",
    "npm": "6.14.12",
    "redis": "3.0.6"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "chai": "*",
    "mocha": "^9.0.3",
    "supertest": "^6.1.5"
  }
}
